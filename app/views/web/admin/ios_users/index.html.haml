%h1 iOS Users

%hr

%table.table.table-striped
  %thead
    %tr
      %th #
      %th Token
      %th State
      %th

  %tbody
    - @ios_users.each do |ios_user|
      %tr
        %td= ios_user.id
        %td= ios_user.token
        %td= ios_user.state
        %td= admin_destroy_button(admin_ios_user_path(ios_user))
